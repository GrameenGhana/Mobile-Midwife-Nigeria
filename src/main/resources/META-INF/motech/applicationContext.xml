<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:annotation-config />
    <context:component-scan base-package="org.motechproject" />
  
    <bean id="moduleRegistrationData" class="org.motechproject.osgi.web.ModuleRegistrationData">
        <constructor-arg name="moduleName" value="mmnaija" />
        <constructor-arg name="url" value="../mmnaija/resources/index.html" />
        <constructor-arg name="angularModules">
            <list>
                <value>mmnaija</value>
            </list>
        </constructor-arg>
        <constructor-arg name="i18n">
            <map>
                <entry key="messages" value="../mmnaija/resources/messages/" />
            </map>
        </constructor-arg>
        <property name="defaultURL" value="/mmnaija"/>
    </bean>
    
    


</beans>
